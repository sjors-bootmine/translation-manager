{
    "name": "open-admin-ext/translation-manager",
    "description": "Translation manager for Open-Admin based on https://github.com/barryvdh/laravel-translation-manager, Thanks Barry",
    "type": "library",
    "keywords": [
        "open-admin",
        "extension",
        "translation",
        "translatable",
        "translations",
        "translation-manager",
        "multi-language"
    ],
    "homepage": "https://github.com/open-admin-org/translation-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Sjors Broersen",
            "email": "sjors@bootmine.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "barryvdh/laravel-translation-manager": "^0.6",
        "open-admin-org/open-admin": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAdmin\\TranslationManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenAdmin\\TranslationManager\\TranslationManagerServiceProvider"
            ]
        }
    }
}